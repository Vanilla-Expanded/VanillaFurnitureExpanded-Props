     <ThingDef ParentName="BuildingBase">
    <defName>VFEPD_AdvancedRotor</defName>
    <label>advanced rotor</label>
    <description>An advanced rotor unit.</description>
    <graphicData>	
      <texPath>Things/Building/PowerProps/AdvancedRotor/AdvancedRotor</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(2,2)</drawSize>
      <shadowData>
        <volume>(0.55, 0.30, 0.40)</volume>
      </shadowData>
    </graphicData>
    <researchPrerequisites><li>PowerProps</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.5</fillPercent>
    <statBases>
      <MaxHitPoints>30</MaxHitPoints>
      <WorkToBuild>50</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>0</Flammability>
    </statBases>
    <size>(2,2)</size>
    <costList>
      <Steel>10</Steel>
    </costList>
    <designationCategory>PropsandDecor</designationCategory>
    <uiIconScale>1</uiIconScale>
    <designatorDropdown>PowerProps</designatorDropdown>
	<building>
		      <isInert>true</isInert>
	</building>
  </ThingDef>
  
