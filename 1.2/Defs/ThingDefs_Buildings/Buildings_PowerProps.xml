     <ThingDef ParentName="BuildingBase">
    <defName>VFEPD_ChemfuelTank</defName>
    <label>large chemfuel tank</label>
    <description>A large tank filled with explosive chemfuel.</description>
    <graphicData>	
      <texPath>Things/Building/PowerProps/ChemfuelTank/ChemfuelTank</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(2,3)</drawSize>
      <shadowData>
        <volume>(0.55, 0.30, 0.40)</volume>
      </shadowData>
    </graphicData>
    <researchPrerequisites><li>PowerProps</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
    <castEdgeShadows>True</castEdgeShadows>
    <tickerType>Normal</tickerType>
    <passability>Impassable</passability>
    <fillPercent>0.5</fillPercent>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Mass>50</Mass>
      <Flammability>0</Flammability>
    </statBases>
    <size>(2,3)</size>
    <costList>
      <Steel>20</Steel>
	  <Chemfuel>30</Chemfuel>
    </costList>
    <designationCategory>PropsandDecor</designationCategory>
    <uiIconScale>1</uiIconScale>
    <designatorDropdown>PowerProps</designatorDropdown>
    <comps>
      <li Class="CompProperties_Explosive">
        <wickTicks>360</wickTicks>
        <explosiveRadius>7.9</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <explodeOnKilled>true</explodeOnKilled>
        <startWickHitPointsPercent>0.3</startWickHitPointsPercent>
        <extraInspectStringKey>ExplosionOnDamageWarning</extraInspectStringKey>
      </li>
    </comps>
	<building>
		      <isInert>true</isInert>
	</building>
  </ThingDef>
  
