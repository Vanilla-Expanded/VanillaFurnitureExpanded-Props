<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_Hive</defName>
		<label>fake hive</label>
		<description>Not a real insect hive.</description>
		<graphicData>
			<texPath>Things/Building/Natural/Hive</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>1.6</drawSize>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>130</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
		<pathCost>42</pathCost>
	</ThingDef>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_SpaceHive</defName>
		<label>space hive</label>
		<description>A decorative space hive. Won't produce any insects.</description>
		<graphicData>
			<texPath>Things/Building/Spaceships/SpaceHive/SpaceHive</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.6</drawSize>
		</graphicData>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>1</Flammability>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
		</statBases>
		<size>(1,1)</size>
		<building>
			<isInert>true</isInert>
			
		</building>
		<fillPercent>0.5</fillPercent>
		<pathCost>42</pathCost>
		
	</ThingDef>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_GlowPod</defName>
		<label>fake glow pod</label>
		<description>This doesn't even glow! I want to speak with the manager!</description>
		<graphicData>
			<texPath>Things/Building/Natural/GlowPod</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.3, 0.6, 0.3)</volume>
				<offset>(0,0,-0.1)</offset>
			</shadowData>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<fillPercent>0.2</fillPercent>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
	
</Defs>
