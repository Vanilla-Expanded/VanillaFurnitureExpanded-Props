<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_AlphaGenesAncientGeneAssembler</defName>
		<label>ancient gene assembler</label>
		<description>A workbench for creating implantable xenogerms from genepacks stored in nearby gene banks. This device has collapsed in on itself, rendering it useless.</description>
		<fillPercent>0.50</fillPercent>
		<graphicData>
			<texPath>Things/Building/AG_GeneAssembler</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,2)</drawSize>
		</graphicData>
		<size>(3,2)</size>
		<defaultPlacingRot>South</defaultPlacingRot>
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Flammability>0.5</Flammability>
		</statBases>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
	</ThingDef>
