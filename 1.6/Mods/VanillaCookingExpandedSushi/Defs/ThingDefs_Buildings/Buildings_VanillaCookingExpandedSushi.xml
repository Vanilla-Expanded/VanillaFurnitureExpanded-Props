<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_SushiPrepTable</defName>
		<label>sushi prep table</label>
		<description>A working table to cut, slice and prepare all kinds of sushi.</description>
		<graphicData>
			<texPath>Things/Buildings/VCE_SushiPrepTable</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<pathCost>42</pathCost>
		<rotatable>true</rotatable>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>15</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(3,1)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_SoyFermenter</defName>
		<label>soy sauce fermenter</label>
		<description>A soy fermenter is a cilindrical, metallic drum where soybeans are pressed, mixed with aspergillus bacteria and grain, and aged for long time periods. This one is inactive and non-functional.</description>
		<graphicData>
			<drawSize>(2,2)</drawSize>
			<texPath>Things/Buildings/VCE_SoyFermenter</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>42</pathCost>
		<fillPercent>0.5</fillPercent>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>10</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(2,2)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
</Defs>
