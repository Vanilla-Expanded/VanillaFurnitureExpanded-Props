<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_Bar</defName>
		<label>decorative bar</label>
		<description>A somewhat ornate countertop stocked to the brim with different vessels and devices for making a variety of drinks.</description>
		<graphicData>
			<texPath>Things/Buildings/VBE_TableBar</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3.5,1.5)</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(2.8, 0.3, 0.54)</volume>
				<offset>(0, 0, -0.18)</offset>
			</shadowData>
			<damageData>
				<rect>(0.3,0,2.7,1)</rect>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<pathCost>42</pathCost>
		<rotatable>true</rotatable>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<MaxHitPoints>180</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>15</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(3,1)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
