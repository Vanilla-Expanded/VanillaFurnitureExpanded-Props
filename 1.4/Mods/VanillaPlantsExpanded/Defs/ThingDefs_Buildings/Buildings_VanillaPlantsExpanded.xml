	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_Beet</defName>
		<thingClass>VFEProps.Building_FakePlant2x2</thingClass>
		<label>beet</label>		
		<description>A fake herb-like vegetable widely cultivated for humans and livestock alike, notably nutritious and historically used in dye making.</description>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Beet</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>0.8</drawSize>
			<drawOffset>(0,0,0.2)</drawOffset>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>Standable</passability>
