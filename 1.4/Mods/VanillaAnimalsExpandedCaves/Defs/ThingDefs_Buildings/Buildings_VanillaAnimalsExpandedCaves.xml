<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_Caves_Cocoon</defName>
		<label>spider cocoon</label>
		<description>Not a real spider cocoon.</description>
		<graphicData>
			<texPath>Things/Building/VAE_WebCocoon</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(1.3,1.3)</drawSize>

		</graphicData>
		<size>(1,1)</size>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>130</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1</Flammability>
		</statBases>
		
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
		<pathCost>42</pathCost>
	</ThingDef>
