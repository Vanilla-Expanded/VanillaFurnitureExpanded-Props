<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_AloeVera_Succulent</defName>
		<label>aloe vera</label>
		<description>A cultivated succulent with thick, greenish, fleshy leaves that fan out from the short stem. This one is fake.</description>
		<graphicData>
			<texPath>Things/Plant/AloeVera</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>1.05</drawSize>
			<drawOffset>(0,0,0.2)</drawOffset>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>Standable</passability>
		<fillPercent>0.1</fillPercent>
		<pathCost>5</pathCost>
		<filthLeaving Inherit="False"/>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
		<clearBuildingArea>false</clearBuildingArea>
	</ThingDef>
