<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
     <ThingDef ParentName="BuildingBase">
    <defName>VFEPD_FoodBox</defName>
    <label>empty food boxes</label>
    <description>A stack of empty food boxes.</description>
    <graphicData>	
      <texPath>Things/Building/FarmProps/FoodBox</texPath>
      <graphicClass>Graphic_Random</graphicClass>
	  <drawSize>(2,2)</drawSize>
    </graphicData>
    <researchPrerequisites><li>FarmProps</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
	<rotatable>false</rotatable>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.1</fillPercent>
	<building>
	      <isInert>true</isInert>
	</building>
    <statBases>
      <MaxHitPoints>20</MaxHitPoints>
      <WorkToBuild>50</WorkToBuild>
      <Mass>5</Mass>
	  <Beauty>1</Beauty>
      <Flammability>1</Flammability>
    </statBases>
    <size>(2,2)</size>
    <costList>
      <WoodLog>5</WoodLog>
    </costList>
    <designationCategory>PropsandDecor</designationCategory>
    <uiIconScale>1</uiIconScale>
    <designatorDropdown>FarmProps</designatorDropdown>
  </ThingDef>
  
