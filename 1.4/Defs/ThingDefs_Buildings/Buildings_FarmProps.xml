<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_FoodBox</defName>
		<label>empty food boxes</label>
		<description>A stack of empty food boxes.</description>
		<graphicData>
			<texPath>Things/Building/FarmProps/FoodBox</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(2,2)</drawSize>
			<color>(255,191,97)</color>
			<shadowData>
				<volume>(1.5,0.5,0.3)</volume>
				<offset>(0,0,-0.2</offset>
			</shadowData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.1</fillPercent>
		<pathCost>42</pathCost>
		<building>
			<isInert>true</isInert>
		</building>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1</Flammability>
		</statBases>
		<size>(2,2)</size>
		<uiIconScale>1</uiIconScale>
	</ThingDef>
