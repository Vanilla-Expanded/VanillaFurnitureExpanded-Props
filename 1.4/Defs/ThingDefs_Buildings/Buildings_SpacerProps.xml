<Defs>
  
    <ThingDef ParentName="BuildingBase">
    <defName>VFEPD_DataConsole</defName>
    <label>data console</label>
    <description>An advanced data console accepting several different types of file formatting.</description>
    <graphicData>	
      <texPath>Things/Building/SpacerProps/DataConsole/DataConsole</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(3,3)</drawSize>
      <shadowData>
        <volume>(0.55, 0.30, 0.40)</volume>
      </shadowData>
    </graphicData>
    <researchPrerequisites><li>SpacerProps</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
	<defaultPlacingRot>South</defaultPlacingRot>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.25</fillPercent>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>50</WorkToBuild>
      <Mass>15</Mass>
	  <Beauty>1</Beauty>
      <Flammability>0</Flammability>
    </statBases>
    <size>(3,1)</size>
    <costList>
      <Steel>10</Steel>
    </costList>
    <designationCategory>PropsandDecor</designationCategory>
    <uiIconScale>1</uiIconScale>
    <designatorDropdown>SpacerProps</designatorDropdown>
	<building>
		      <isInert>true</isInert>
	</building>
  </ThingDef>
  
