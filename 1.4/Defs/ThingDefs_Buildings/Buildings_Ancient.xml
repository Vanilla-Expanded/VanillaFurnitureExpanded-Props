	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_AncientConcreteBarrier</defName>
		<label>ancient concrete barrier</label>
		<description>A traffic management solution from long ago.</description>
		<graphicData>
			<texPath>Things/Building/Ruins/AncientConcreteBarrier</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<color>(231,216,206)</color>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.35</fillPercent>
		<pathCost>42</pathCost>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>6</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_AncientLamppost</defName>
		<label>ancient lamppost</label>
		<description>Road lighting from long ago. No longer functional.</description>
		<graphicData>
			<texPath>Things/Building/Ruins/AncientLamppost</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(1,2)</drawSize>
			<color>(231,216,206)</color>
