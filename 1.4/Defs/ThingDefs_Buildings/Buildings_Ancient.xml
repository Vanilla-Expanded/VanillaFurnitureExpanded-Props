<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_ShipChunk</defName>
		<label>ship chunk</label>
		<description>A chunk of a spacecraft. Upon closer inspection it seems to be made of painted cardboard.</description>
		<graphicData>
			<texPath>Things/Building/Exotic/ShipChunk</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rect>(0.1,0.1,1.8,1.8)</rect>
			</damageData>
			<shadowData>
				<volume>(1.39,0.5,1.25)</volume>
			</shadowData>
		</graphicData>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Flammability>0.8</Flammability>
			<WorkToBuild>0</WorkToBuild>
			<Mass>20</Mass>
		</statBases>
		<size>(2,2)</size>
		<rotatable>false</rotatable>
		<building>
			<isInert>true</isInert>
		</building>
		<fillPercent>0.5</fillPercent>
		<pathCost>42</pathCost>
	</ThingDef>
