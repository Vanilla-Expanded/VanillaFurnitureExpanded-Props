<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_SteelTank</defName>
		<label>large steel tank</label>
		<description>A large steel tank.</description>
		<graphicData>
			<texPath>Things/Building/PowerProps/SteelTank/SteelTank</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,3)</drawSize>
			<shadowData>
				<volume>(0.55, 0.30, 0.40)</volume>
			</shadowData>
			<color>(105,105,105)</color>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<castEdgeShadows>True</castEdgeShadows>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>50</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(2,3)</size>
		<uiIconScale>1</uiIconScale>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_ChemfuelTank</defName>
		<label>large chemfuel tank</label>
		<description>A large tank filled with explosive chemfuel.</description>
		<graphicData>
			<texPath>Things/Building/PowerProps/ChemfuelTank/ChemfuelTank</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,3)</drawSize>
			<shadowData>
				<volume>(0.55, 0.30, 0.40)</volume>
			</shadowData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<castEdgeShadows>True</castEdgeShadows>
		<tickerType>Normal</tickerType>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>50</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(2,3)</size>
		<uiIconScale>1</uiIconScale>
		<comps>
			<li Class="CompProperties_Explosive">
				<wickTicks>360</wickTicks>
				<explosiveRadius>7.9</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explodeOnKilled>true</explodeOnKilled>
				<startWickHitPointsPercent>0.3</startWickHitPointsPercent>
				<extraInspectStringKey>ExplosionOnDamageWarning</extraInspectStringKey>
			</li>
		</comps>
		<building>
			<isInert>true</isInert>
		</building>
	</ThingDef>
