<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>VFEPD_CarForklift</defName>
    <label>forklift</label>
    <description>An industrial style forklift.</description>
    <graphicData>	
      <texPath>Things/Building/Vehicles/CarForklift/CarForklift</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(3,5)</drawSize>
      <shadowData>
        <volume>(0.55, 0.30, 0.40)</volume>
      </shadowData>
    </graphicData>
    <tickerType>Normal</tickerType>
    <researchPrerequisites><li>VehicleProps</li></researchPrerequisites>
    <altitudeLayer>Building</altitudeLayer>
    <defaultPlacingRot>South</defaultPlacingRot>
    <castEdgeShadows>True</castEdgeShadows>
    <blockWind>true</blockWind>
    <passability>Impassable</passability>
    <fillPercent>0.5</fillPercent>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <repairEffect>ConstructMetal</repairEffect>
    <statBases>
      <MaxHitPoints>450</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>200</Mass>
      <Flammability>0.2</Flammability>
    </statBases>
    <size>(2,4)</size>
    <costList>
      <Steel>100</Steel>
	  <Chemfuel>20</Chemfuel>
    </costList>
    <designationCategory>PropsandDecor</designationCategory>
    <uiIconScale>1</uiIconScale>
    <designatorDropdown>VehicleProps</designatorDropdown>
    <comps>
      <li Class="CompProperties_Explosive">
        <wickTicks>360</wickTicks>
        <explosiveRadius>7.9</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <explodeOnKilled>true</explodeOnKilled>
        <startWickHitPointsPercent>0.3</startWickHitPointsPercent>
        <extraInspectStringKey>ExplosionOnDamageWarning</extraInspectStringKey>
      </li>
    </comps>
  </ThingDef>
  
