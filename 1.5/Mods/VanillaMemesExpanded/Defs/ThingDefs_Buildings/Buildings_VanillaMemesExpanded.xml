<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_SpaceJunk</defName>
		<label>space junk</label>
			<description>Ancient chunks of space junk that fell from orbit. Perhaps a relic of an ancient space battle.</description>
		<graphicData>
			<texPath>Things/Building/Misc/AncientSpaceJunk</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			
			<damageData>
				<rect>(0.1,0.1,1.8,1.8)</rect>
			</damageData>
			<shadowData>
				<volume>(1.39,0.5,1.25)</volume>
			</shadowData>
		</graphicData>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Flammability>0.8</Flammability>
			<WorkToBuild>0</WorkToBuild>
			<Mass>20</Mass>
		</statBases>
		<size>(1,1)</size>
		<rotatable>false</rotatable>
		<building>
			<isInert>true</isInert>
		</building>
		<fillPercent>0.5</fillPercent>
		<pathCost>42</pathCost>
	</ThingDef>
	
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
		<defName>VFEPD_Tentacle</defName>
		<label>tentacle</label>

		<description>This is a fleck prop for "Tentacle". Intrinsically laggy, use in moderation.</description>
		<graphicData>
			<texPath>Things/Building/ModCompat/Empty</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>MoteOverhead</altitudeLayer>
		<passability>Standable</passability>
		<pathCost>0</pathCost>
		<tickerType>Normal</tickerType>
		<drawerType>MapMeshOnly</drawerType>
		<uiIconPath>Motes/VME_TentacleMote</uiIconPath>
		<building>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
			<isEdifice>false</isEdifice>
			<isInert>true</isInert>
		</building>
		<castEdgeShadows>false</castEdgeShadows>
		<fillPercent>0</fillPercent>
		<statBases>
			<MaxHitPoints>5</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>0.1</Mass>
			<Flammability>0.1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
		<clearBuildingArea>false</clearBuildingArea>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<comps>
			<li Class="CompProperties_FleckEmitter">
				<fleck>VME_Tentacle</fleck>
				<emissionInterval>27</emissionInterval>
			</li>
		</comps>
	</ThingDef>
