<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
    <defName>VFEPD_VFEI2_SmallBurrow</defName>
    <label>small insect burrow</label>
    <description>A small insectoid-made hole connecting to some unseen cave network deep under the surface. The walls are covered in insectoid goop and are too slippery to climb down. Seems to be empty inside.</description>
    <graphicData>
        <texPath>Things/Building/InsectBurrowSmall</texPath>
        <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <blockWind>false</blockWind>
    <passability>Impassable</passability>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <repairEffect>ConstructMetal</repairEffect>
    <statBases>
        <MaxHitPoints>150</MaxHitPoints>
        <WorkToBuild>0</WorkToBuild>
        <Mass>35</Mass>
        <Flammability>1</Flammability>
    </statBases>
    <size>(1,1)</size>
    <rotatable>false</rotatable>
    <building>
        <isInert>true</isInert>
    </building>
</ThingDef>

