<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_Balloon</defName>
		<label>Balloon</label>
		<description>A lighter-than-air aircraft consisting of a bag, called an envelope, which contains heated air. Non-functional: all its internal mechanisms have been removed.</description>
		<graphicData>
			<texPath>Things/Vehicles/Air/Tier0/Balloon/AV_Balloon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(15,15)</drawSize>
			<color>(77, 35, 25)</color>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(0.7, 0.7, 1)</volume>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Vehicles/Air/Tier0/Balloon/AV_BalloonUI</uiIconPath>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		
		<blockWind>true</blockWind>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<repairEffect>ConstructMetal</repairEffect>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>200</Mass>
			<Flammability>1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
	</ThingDef>
	
