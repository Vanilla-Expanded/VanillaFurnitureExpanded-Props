<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_NonColourableFurnitureBase">
	<defName>VFEPD_AM_Exit_L</defName>
	<label>Ancient escalator</label>
	<description>An upward escalator that has ceased operation. Non-functional: all its internal mechanisms have been removed.</description>
	<graphicData>
		<graphicClass>Graphic_Multi</graphicClass>
		<texPath>Things/Building/AscendingEscalator</texPath>
		<drawSize>7</drawSize>
	</graphicData>
	<altitudeLayer>BuildingOnTop</altitudeLayer>
	<blockWind>true</blockWind>
	<passability>PassThroughOnly</passability>
	<pathCost>42</pathCost>
	<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
	<repairEffect>ConstructMetal</repairEffect>
	<statBases>
		<MaxHitPoints>100</MaxHitPoints>
		<WorkToBuild>0</WorkToBuild>
		<Mass>200</Mass>
		<Flammability>1</Flammability>
	</statBases>
	<size>(4,4)</size>
	<rotatable>true</rotatable>
	<building>
		<isInert>true</isInert>
	</building>
</ThingDef>

