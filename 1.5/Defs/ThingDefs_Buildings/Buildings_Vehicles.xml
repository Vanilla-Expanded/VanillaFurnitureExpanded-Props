<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_CarForklift</defName>
		<label>forklift</label>
		<description>An industrial style forklift. Non-functional: all its internal wiring and its motor have been removed.</description>
		<graphicData>
			<texPath>Things/Building/Vehicles/CarForklift/CarForklift</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,5)</drawSize>
			<shadowData>
				<volume>(1.8, 0.7, 3.8)</volume>
			</shadowData>
			<color>(252,244,134)</color>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<defaultPlacingRot>South</defaultPlacingRot>
		<blockWind>true</blockWind>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<repairEffect>ConstructMetal</repairEffect>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>200</Mass>
			<Flammability>0.2</Flammability>
		</statBases>
		<size>(2,4)</size>
		<uiIconScale>1</uiIconScale>
	</ThingDef>
